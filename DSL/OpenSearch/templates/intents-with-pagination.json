{
  "script": {
    "lang": "mustache",
    "source": {
      "from": "{{from}}",
      "size": "{{size}}",
      "track_total_hits": true,
      "_source": ["intent"],
      "query": {
        "regexp": {
          "intent": {
            "value": ".*{{filter}}.*",
            "case_insensitive": true,
            "flags": "ALL"
          }
        }
      }
    },
    "params": {
      "from": 0,
      "size": 10,
      "filter": ""
    }
  }
}
