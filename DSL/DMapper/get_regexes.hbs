{
  "regexes": [
    {{#each hits}}
      {
       "{{_source.regex}}"{{#if ../examples}}: ,
        [
            {{#each _source.examples}}
            "{{this}}"{{#unless @last}},{{/unless}}
            {{/each}}
            ]
       {{/if}}
      }{{#unless @last}},{{/unless}}
    {{/each}}
  ]
}
