{
  "regexes": [
    {{#each hits}}
      {
       "regex": "{{_source.regex}}"{{#if ../examples}},
        "examples": [
            {{#each _source.examples}}
            "{{this}}"{{#unless @last}},{{/unless}}
            {{/each}}
            ]
       {{/if}}
      }{{#unless @last}},{{/unless}}
    {{/each}}
  ]
}
