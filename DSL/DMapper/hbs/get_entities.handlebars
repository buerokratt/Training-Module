{
    "entities": [
    {{#each data.hits}}
        {{#if (eq this._id "entities") }}
            {{#each this._source.entities}}
                {
                  "id": "{{{ this }}}",
                  "name": "{{{ this }}}"
                }
                {{#unless @last}},{{/unless}}
            {{/each}}
        {{/if}}
    {{/each}}
    ]
}
