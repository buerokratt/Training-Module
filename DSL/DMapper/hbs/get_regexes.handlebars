{
  "regexes": [
    {{#each hits}}
       {{#if ../examples}}{"{{_source.regex}}":
        [
          {{#each _source.examples}}
            "{{this}}"{{#unless @last}},{{/unless}}
          {{/each}}
        ]
        }
      {{else}}
           {
            "name":"{{_source.regex}}",
            "id":"{{_source.regex}}"
           }
       {{/if}}{{#unless @last}},{{/unless}}
    {{/each}}
  ]
}
