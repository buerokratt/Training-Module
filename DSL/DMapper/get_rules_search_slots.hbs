{
  "rules": [
    {{#each hits}}
      {{#if _source.condition.slot_was_set}}
        "{{_source.rule}}"
      {{else}}
        {{#each _source.steps as |step|}}
          {{#if step.slot_was_set}}
            "{{_source.rule}}"
          {{/if}}
        {{/each}}
        {{#unless @last}},{{/unless}}
      {{/if}}
    {{/each}}
  ]
}
