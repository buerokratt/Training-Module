{
    "{{ concatStringIfAbsent form.name '_form'}}": {
    "required_slots": [
            {{#each form.required_slots }}
                "{{this}}"{{#unless @last}},{{/unless}}
            {{/each}}
        ],
    "ignored_intents": [
            {{#each form.ignored_intents }}
                "{{this}}"{{#unless @last}},{{/unless}}
            {{/each}}
        ]
    }
}