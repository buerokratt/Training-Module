{
    "slots": [
        {{#each slots}}
            {{#each (extractSlotKeys _source.slots)}}
                {
                    "id":"{{this}}",
                    "name":"{{this}}",
                    "text": "{{ findMatchInObject ../../responses this 'utter_ask_' }}"
                }{{#unless @last}},{{/unless}}
            {{/each}}{{#unless @last}},{{/unless}}
        {{/each}}
    ]
}